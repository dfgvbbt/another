print('Thanks')
local All_Items_Name = {
	['Block'] = true,
	['Food'] = true,
	['Fuse'] = true,
	['Battery'] = true,
}

--Block.TouchTrigger.TouchInterest
--workspace.GroupBuildStructures.BlockTower.Trigger.TouchInterest
local firetouchinterest

local touched

do
	
	firetouchinterest = function(part1:Part, part2:Part)
		if (part1 and part2) then
			touched = part1.CFrame
			part1.CFrame = part2.CFrame
			wait()
			part1.CFrame = touched
		end
	end
end

local function Doing(child:Instance)
	local Part = child:WaitForChild('TouchTrigger')
	firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild('HumanoidRootPart'),Part,0)
	Part = workspace:WaitForChild('GroupBuildStructures'):FindFirstChildWhichIsA('Model'):WaitForChild('Trigger')
	firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild('HumanoidRootPart'),Part)
	
end

workspace.ChildAdded:Connect(function(child)
	if child:IsA('Model') then
		if All_Items_Name[child.Name] ~= nil then
			Doing(child)
		else
			for Name,v in pairs(All_Items_Name) do
				if string.match(child.Name,Name) then
					Doing(child)
				end
			end
		end
		
	end
end)
