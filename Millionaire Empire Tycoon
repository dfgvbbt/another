local limited = (9)
getgenv().Money = true
spawn(function()
    while Money == true do
        if not Money or limited < 1 then break end
        local args = {
            [1] = 1000000000000000000,
        }

        game:GetService("ReplicatedStorage").RespawnService:FireServer(unpack(args))
        limited = limited - 1
        print(limited.." charges left")
        wait(0)
        if limited < 1 then
             print ("Maxed!, now lets collect your money from cash collector!")
        end
    end
end)
