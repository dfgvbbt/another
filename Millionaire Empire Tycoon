local limited = (9)
getgenv().Money = true
spawn(function()
    while Money == true do
        if not Money or limited < 1 then break end
        local args = {
            [1] = 1000000000000000000,
        }

        game:GetService("ReplicatedStorage").RespawnService:FireServer(unpack(args))
        limited = limited - 1
        print(limited.." charges left")
        wait(0)
        if limited < 1 then
             print ("Maxed!, now lets collect your money from cash collector!")
             print ("executing gui...")
        end
    end
end)
wait (0.3)
_G.RedGUI = false
_G.Theme = "Dark" -- Must disable or remove _G.RedGUI to use
--Themes: Light, Dark, Mocha, Aqua and Jester

loadstring(game:HttpGet("https://raw.githubusercontent.com/CasperFlyModz/discord.gg-rips/main/MillionaireEmpireTycoon.lua"))()
